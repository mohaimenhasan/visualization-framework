{
    "id": "inf-regress-utilization-users-details",
    "title": "Utilization by a user",
    "service": "elasticsearch",
    "query": {
        "index": "{{index:nuage_regress}}",
        "type": "{{type:nuage_doc_type}}",
        "body": {
            "size": 0,
            "query": {
                "bool": {
                    "must": [
                        {
                            "term": {
                                "regression_req_by": "{{users}}"
                            }
                        },
                        {
                            "range":{
                                "time_regression_submitted":{
                                    "gte":"{{startTime:now-24h}}",
                                    "lte":"{{endTime:now}}",
                                    "format":"epoch_millis"
                                }
                            }
                        }
                    ]
                }
            },
            "aggs": {
                "testbed": {
                    "terms": {
                        "field": "testbed",
                        "size": 100
                    }
                }
            }
        }
    }
}